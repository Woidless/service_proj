{
    "version": 2,
    "build": [
        {
            "src": "backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        },
        {
            "src":"buil.sh",
            "use": "@vaercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
}